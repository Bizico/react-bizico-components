{
  "name": "react-bizico-components",
  "version": "1.0.0",
  "private": true,
  "description": "React components build by Bizico team.",
  "main": "index.js",
  "files": [
    "es",
    "umd"
  ],
  "global": "bComponents",
  "repository": "https://github.com/Bizico/react-bizico-components.git",
  "author": "Maksym Ridush <mridush@bizico.com>",
  "license": "MIT",
  "scripts": {
    "setup": "yarn || npm install",
    "start": "webpack-dev-server --config ./scripts/webpack/demo.config.js",
    "build": "webpack --config ./scripts/webpack/main.config.js",
    "lint": "esw src; exit 0",
    "test": "jest -u --no-cache --coverage",
    "test:watch": "jest -u --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "extract-text-webpack-plugin": "2.0.0-rc.2",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.1.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.2.2",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "next",
    "react-test-renderer": "^15.4.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^1.16.3",
    "webpack-merge": "^2.6.1"
  },
  "peerDependencies": {
    "react-bootstrap": "^0.30.7"
  }
}
