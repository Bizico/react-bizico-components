{
  "presets": [
    [
      "es2015",
      {
        "modules": false
      }
    ],
    "es2016",
    "stage-2",
    "react"
  ],
  "plugins": [
    "transform-react-jsx",
    "react-hot-loader/babel",
    "transform-object-rest-spread"
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  },
  "comments": false
}